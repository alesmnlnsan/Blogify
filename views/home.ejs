<div class="nav-list">
    <a href="/">Home</a>
    <a href="/about">About</a>
    <a href="/contact">Contact Us</a>
  </div>
  
  <h1>Home dashboard</h1>
  
  <% if (posts.length === 0) { %>
    <p>No posts found.</p>
  <% } else { %>
    <ul>
      <% posts.forEach((post) => { %>
        <li>
          <h2><%= post.title %></h2>
          <p><%= post.content %></p>
          <p><strong>Author:</strong> <%= post.author %></p>
          <p><strong>Publication Date:</strong> <%= post.publicationDate %></p>
          <img src="<%= post.image_url %>" alt="">
        </li>
      <% }) %>
    </ul>
  <% } %>
  